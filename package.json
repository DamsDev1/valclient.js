{
    "name": "valclient.js",
    "version": "1.0.0",
    "description": "Valorant Client API Wrapper in NodeJS",
    "main": "index.js",
    "scripts": {
        "lint": "eslint '*/**/*.ts' --quiet --fix",
        "test": "jest",
        "prepare": "husky install",
        "start": "cross-env NODE_ENV=development nodemon --watch src -r tsconfig-paths/register src/index.ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/igorwessel/valclient.js.git"
    },
    "author": "Igor Wessel",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/igorwessel/valclient.js/issues"
    },
    "homepage": "https://github.com/igorwessel/valclient.js#readme",
    "dependencies": {
        "axios": "0.21.4",
        "axios-cookiejar-support": "1.0.1",
        "tough-cookie": "4.0.0",
        "yaml": "1.10.2"
    },
    "devDependencies": {
        "@types/jest": "27.0.1",
        "@types/node": "16.7.13",
        "@types/tough-cookie": "4.0.1",
        "@typescript-eslint/eslint-plugin": "4.31.0",
        "@typescript-eslint/parser": "4.31.0",
        "cross-env": "7.0.3",
        "dotenv": "10.0.0",
        "eslint": "7.32.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-prettier": "4.0.0",
        "husky": "7.0.2",
        "jest": "27.1.1",
        "lint-staged": "11.1.2",
        "nodemon": "2.0.12",
        "prettier": "2.3.2",
        "ts-jest": "27.0.5",
        "ts-node": "10.2.1",
        "tsconfig-paths": "3.11.0",
        "typescript": "4.4.2"
    },
    "lint-staged": {
        "*.ts": [
            "eslint --fix"
        ],
        "*.js": "eslint --cache --fix"
    }
}
